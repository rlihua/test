page {
    background: #f5f5f5; }
page .order-table-layer {
    width: 100%;
    height: 100%; }
page .order-table-layer .order-table-body {
    background: #f5f5f5;
    margin-top: 25rpx; }
page .order-table-layer .order-table-body .order-table-each-info {
    background: #fff;
    padding: 0rpx 25rpx;
    margin: 0rpx auto 25rpx; }
page .order-table-layer .order-table-body .order-table-each-info .order-table-top {
    height: 100rpx;
    vertical-align: middle;
    border-bottom: 1px solid #eee;
    padding: 25rpx 0rpx; }
page .order-table-layer .order-table-body .order-table-each-info .order-table-top image {
    width: 100rpx;
    height: 100rpx;
    border: 1px solid #ddd; }
page .order-table-layer .order-table-body .order-table-each-info .order-table-top .order-store-name {
    font-size: 32rpx;
    font-weight: 600;
    vertical-align: top;
    display: inline-block;
    line-height: 100rpx;
    height: 100rpx;
    margin-left: 20rpx; }
page .order-table-layer .order-table-body .order-table-each-info .order-table-content {
    font-size: 30rpx;
    border-bottom: 1px solid #eee;
    padding-top: 5rpx;
    padding-bottom: 25rpx; }
page .order-table-layer .order-table-body .order-table-each-info .order-table-content .order-table-each {
    display: -webkit-flex;
    /* Safari */
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding-top: 10rpx;
    color: #666; }
page .order-table-layer .order-table-body .order-table-each-info .order-table-content .order-table-each .order-table-none {
    width: 120rpx; }
page .order-table-layer .order-table-body .order-table-each-info .order-table-content .order-table-each .order-table-label {
    flex-grow: 1; }
page .order-table-layer .order-table-body .order-table-each-info .order-table-content .order-table-each .order-table-value {
    flex-grow: 1;
    text-align: right; }
page .order-table-layer .order-table-body .order-table-each-info .order-table-footer {
    padding: 25rpx 0rpx;
    height: 70rpx;
    text-align: right; }
page .order-table-layer .order-table-body .order-table-each-info .order-table-footer .order-table-dis-btn,.del-order-btn {
    background: #eee;
    border: 1px solid #ddd;
    border-radius: 6rpx;
    height: 62rpx;
    font-size: 30rpx;
    display: inline-block;
    line-height: 62rpx;
    width: 180rpx;
    text-align: center; }
page .order-table-layer .no-data-view {
    text-align: center;
    font-size: 30rpx;
    color: #bfbfbf;
    padding: 80rpx 0rpx 0rpx; }
page .order-table-layer .no-data-view image {
    width: 153px;
    height: 100px; }

.loading {
    padding: 5px 0;
    font-size: 14px;
    color: #666; }
.success-status{
    color:#0BB20C;
}
.failed-status{
    color: #F43530;
}

